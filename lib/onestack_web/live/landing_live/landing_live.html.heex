    <!-- App Mockups Interactive Display -->
    <div class="relative max-w-6xl mx-auto mt-12 perspective-1000">
      <div class="flex flex-col lg:flex-row items-center justify-center gap-8 mb-12">
        <!-- Floating App Icons -->
        <%= for {product, index} <- Enum.with_index(Onestack.CatalogMonthly.list_products()) do %>
          <% # Calculate total number of products
          total_products =
            length(Onestack.CatalogMonthly.list_products())

          # Define fixed positions that will be used in rotation
          positions = [
            "top-4 left-8",
            "top-6 right-6",
            "bottom-40 left-10",
            "bottom-24 right-16",
            "top-22 -left-20",
            "top-32 right-8",
            "bottom-16 left-8",
            "bottom-42 right-10"
          ]

          # Add more positions if needed based on product count
          extended_positions =
            positions ++
              if total_products > 8 do
                [
                  "top-40 left-2",
                  "top-24 right-24",
                  "bottom-12 left-12",
                  "bottom-20 right-28",
                  "top-28 left-20",
                  "top-12 right-12",
                  "bottom-28 left-28",
                  "bottom-4 right-8"
                ]
              else
                []
              end

          # Calculate a deterministic position index that scales with product count
          position_index = rem(index, length(extended_positions))

          # Animations with better distribution
          animations = [
            "animate-bounce-slow",
            "animate-bounce-delayed",
            "animate-float",
            "animate-float-delayed"
          ]

          # Generate a unique color for each product based on its index
          colors = [
            "text-blue-500 hover:text-blue-600"
          ]

          color_class = Enum.at(colors, rem(index, length(colors))) %>
          <div class={"absolute #{Enum.at(extended_positions, position_index)} #{Enum.at(animations, rem(index, length(animations)))} opacity-70 hidden md:block group"}>
            <div
              class="w-12 h-12 rounded-md shadow-lg bg-white dark:bg-gray-800 flex items-center justify-center 
               transform transition-all duration-300 hover:scale-110 hover:shadow-xl 
               hover:ring-2 hover:ring-offset-2 hover:ring-offset-white dark:hover:ring-offset-gray-900 hover:ring-blue-400"
              title={product.display_name}
            >
              <Lucide.render
                icon={product.icon_name}
                class={"w-6 h-6 #{color_class} transition-all duration-300 group-hover:scale-110"}
              />
              <div class="absolute opacity-0 group-hover:opacity-100 transition-opacity duration-300 -bottom-2 translate-y-full bg-gray-900 text-white text-xs rounded px-2 py-1 whitespace-nowrap">
                <%= product.display_name %>
              </div>
            </div>
          </div>
        <% end %>
